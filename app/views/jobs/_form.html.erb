<%= simple_form_for(@job) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :time %>
    <%= f.input :finished %>
    <%= f.input :order %>
    <%= f.association :multi %>
    <%= f.input :distance %>
    <%= f.input :checkin %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
