<h1>Listing jobs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Time</th>
      <th>Finished</th>
      <th>Order</th>
      <th>Multi</th>
      <th>Distance</th>
      <th>Checkin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.user %></td>
        <td><%= job.time %></td>
        <td><%= job.finished %></td>
        <td><%= job.order %></td>
        <td><%= job.multi %></td>
        <td><%= job.distance %></td>
        <td><%= job.checkin %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
